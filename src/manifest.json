{
  "manifest_version": 3,
  "name": "Codexty Email Extractor",
  "description": "A chrome extension that extracts emails from gmail for the name, website provided in a csv file",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-1024.png"
  },
  "icons": {
    "128": "icon-1024.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "icon-128.png", "icon-34.png"],
      "matches": []
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
